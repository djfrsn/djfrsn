{
  "name": "@trinityresearch/app",
  "version": "0.1.4",
  "description": "Build new apps quickly",
  "main": "lib/index.ts",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "NODE_OPTIONS='--no-experimental-fetch' next",
    "build": "npm run setup && next build",
    "build:dev": "npm run setup:dev && next build",
    "start": "next start",
    "setup": "npm run generate && npm run migrate:prisma",
    "setup:dev": "npm run generate && npm run migrate:prisma:dev",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "db:reset": "npx prisma migrate reset",
    "migrate:prisma": "prisma migrate deploy",
    "migrate:prisma:dev": "prisma migrate dev --name init",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "studio": "npx prisma studio",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "slicemachine": "start-slicemachine",
    "publish": "npm publish --access public",
    "log": "heroku logs -a blockwizards",
    "log:tail": "heroku logs -a blockwizards  --tail",
    "bull-board": "ts-node -r dotenv/config lib/bull-board"
  },
  "dependencies": {
    "@apollo/client": "3.6.4",
    "@bull-board/ui": "^4.0.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@prisma/client": "^3.15.2",
    "@prismicio/client": "^6.4.3",
    "@prismicio/helpers": "^2.3.0",
    "@prismicio/next": "^0.1.2",
    "@prismicio/react": "^2.3.0",
    "@prismicio/slice-simulator-react": "^0.2.2",
    "apollo-server-micro": "3.7.0",
    "bullmq": "^1.86.2",
    "chart.js": "^3.8.0",
    "classnames": "^2.3.1",
    "color": "^4.2.3",
    "dotenv": "^16.0.1",
    "express": "4.18.1",
    "got": "^12.1.0",
    "graphql": "15.8.0",
    "graphql-scalars": "1.17.0",
    "graphql-tag": "2.12.6",
    "i": "^0.3.7",
    "is-absolute-url": "^4.0.1",
    "lodash": "^4.17.21",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "moment": "^2.29.3",
    "moment-business-days": "^1.2.0",
    "nexus": "1.3.0",
    "npm": "^8.13.1",
    "p-queue": "^7.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.2"
  },
  "devDependencies": {
    "@bull-board/express": "^4.0.0",
    "@types/graphql": "14.5.0",
    "@types/micro-cors": "0.1.2",
    "@types/node": "16.11.36",
    "@types/react": "18.0.9",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "next": "^12.1.6",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "prisma": "^3.15.2",
    "slice-machine-ui": "^0.3.8",
    "tailwindcss": "^3.0.24",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djfrsn/trinity-app.git"
  },
  "author": "dennismjefferson@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djfrsn/trinity-app/issues"
  },
  "keywords": [
    "typescripts",
    "react",
    "next.js",
    "graphql",
    "fullstack",
    "application",
    "development",
    "boilerplate"
  ],
  "engines": {
    "node": "~18.4.0",
    "npm": "~8.12.1"
  },
  "homepage": "https://github.com/djfrsn/trinity-app#readme"
}
